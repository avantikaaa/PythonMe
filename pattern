def  alpha_tri(n):        #defining function which create a triangle having 
    num = 65              #starting with ASCII value of 'A'
    for i in range(0, n):        # -for- loop for changing row 
        for j in range(0, i+1):  # -for- loop for elements in the row 
          ch = chr(num)
          print(ch, end=" ")
          num = num +1           # increasing the number
        print("\r")              
        
        
height_of_triangle = input()     # getting input
alpha_tri(height_of_triangle)    # calling the function
